{
    "i18n-ally.localesPaths": [
        "public/locales",
        "src/shared/config/i18n"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "stylelint.enable": true,
    "css.validate": true,
    "scss.validate": true,
    "stylelint.validate": [
        "css",
        "scss"
    ],
    "i18n-ally.keystyle": "flat",
}